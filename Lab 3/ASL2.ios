conf t
hostname ALS2

vtp mode server
vtp domain SWLAB
vtp version 2

vlan 99
  name MANAGEMENT
vlan 100
  name SERVERS
vlan 110
  name GUEST
vlan 120
  name OFFICE
vlan 999
  name PARKING_LOT
  state suspend
vlan 666
  name NATIVE_DO_NOT_USE
exit

int range fa0/1-10
  switchport trunk native vlan 666
  switchport trunk allowed vlan except 1,999
  switchport mode trunk
  switchport nonegotiate
  no shutdown
exit

int vlan99
  ip address 10.1.99.104 255.255.255.0
  no shutdown
exit

spanning-tree mode mst
vtp version 3
vtp mode client mst

int fa0/7
  spanning-tree mst 2 cost 1000
exit
